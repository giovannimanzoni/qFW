    "sebastian/phpcpd": "*",
    "phpmd/phpmd" : "@stable",
    "pdepend/pdepend" : "2.5.0",
    "behat/behat": "3.4.3",
    "symfony/yaml": "v3.3.14",
    "wimg/php-compatibility": "*",
    "dealerdirect/phpcodesniffer-composer-installer": "*",
    "object-calisthenics/phpcs-calisthenics-rules": "*"
