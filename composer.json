{
  "name": "webtools/qFW",
  "description": "Framewotk with php class utility for build professional website",
  "keywords": ["qFW", "framework"],
  "type": "framework",
  "authors": [
    {
      "name": "Giovanni Manzoni",
      "email": "ask@giovannimanzoni.com",
      "homepage": "https://giovannimanzoni.com"
    }
